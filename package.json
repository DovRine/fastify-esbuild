{
  "name": "fastify-esbuild",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "tsx watch src/server/app",
    "build": "npm run clean && tsx esbuild",
    "start": "npm run build && node dist/server/app",
    "posttest": "node ./test/vitest.js off",
    "test": "vitest",
    "pretest": "node ./test/vitest.js on",
    "test:coverage": "npm run pretest && vitest run --coverage && npm run posttest"
  },
  "author": "Dov Rine",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vitest/coverage-c8": "^0.28.5",
    "cjstoesm": "^2.1.2",
    "esbuild": "^0.17.9",
    "esbuild-plugin-pino": "1.2.8",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "json-schema-to-ts": "2.6.2",
    "pino": "8.10.0",
    "pino-pretty": "^9.3.0",
    "prettier": "2.8.4",
    "rimraf": "^4.1.2",
    "standard-version": "9.5.0",
    "thread-stream": "2.3.0",
    "tiny-glob": "0.2.9",
    "tsx": "3.12.3",
    "typescript": "4.9.5",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "@fastify/autoload": "5.7.1",
    "@fastify/sensible": "5.2.0",
    "@fastify/swagger": "8.3.1",
    "@fastify/swagger-ui": "^1.3.0",
    "dotenv": "^16.0.3",
    "fastify": "4.13.0",
    "fastify-plugin": "4.5.0",
    "uuid": "^9.0.0"
  }
}
